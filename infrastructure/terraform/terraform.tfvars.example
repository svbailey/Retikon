project_id        = "simitor"
region            = "us-central1"
env               = "dev"
raw_bucket_name   = "retikon-raw-simitor-dev"
graph_bucket_name = "retikon-graph-simitor-dev"
artifact_repo_name = "retikon-repo"
firestore_location = "nam5"
snapshot_uri      = "gs://retikon-graph-simitor-dev/retikon_v2/snapshots/retikon.duckdb"
raw_prefix        = "raw"
duckdb_skip_healthcheck = true
index_builder_work_dir = "/tmp"
index_builder_copy_local = false
index_builder_fallback_local = true
max_frames_per_video = 600
max_ingest_attempts = 5
rate_limit_doc_per_min = 60
rate_limit_image_per_min = 60
rate_limit_audio_per_min = 20
rate_limit_video_per_min = 10
ingestion_concurrency = 1
ingestion_max_scale = 10
query_concurrency = 10
query_max_scale = 20
query_min_scale = 1
audit_concurrency = 10
audit_max_scale = 10
audit_min_scale = 0
audit_require_admin = true
dev_console_concurrency = 10
edge_gateway_concurrency = 10
edge_gateway_max_scale = 20
stream_ingest_concurrency = 10
stream_ingest_max_scale = 20
dev_console_service_name = "retikon-dev-console"
audit_service_name = "retikon-audit"
edge_gateway_service_name = "retikon-edge-gateway"
stream_ingest_service_name = "retikon-stream-ingest"
eventarc_transport_topic_name = "retikon-ingest-transport"
ingest_dlq_topic_name = "retikon-ingest-dlq"
ingest_dlq_subscription_name = "retikon-ingest-dlq-sub"
stream_ingest_topic_name = "retikon-stream-ingest"
stream_ingest_subscription_name = "retikon-stream-ingest-sub"
stream_ingest_batch_max = 50
stream_ingest_batch_max_delay_ms = 2000
stream_ingest_backlog_max = 1000
stream_ingest_max_delivery_attempts = 10
stream_ingest_retry_min_backoff = 10
stream_ingest_retry_max_backoff = 600
compaction_schedule = "0 * * * *"
compaction_schedule_timezone = "Etc/UTC"
compaction_target_min_bytes = 104857600
compaction_target_max_bytes = 1073741824
compaction_max_groups_per_batch = 50
compaction_delete_source = false
compaction_dry_run = false
compaction_strict = true
retention_hot_days = 0
retention_warm_days = 30
retention_cold_days = 180
retention_delete_days = 0
retention_apply = false
alert_notification_channels = []
alert_notification_emails = ["alerts@example.com"]
alert_ingest_5xx_rate = 0.02
alert_query_p95_seconds = 2.0
alert_dlq_backlog = 1
monitoring_dashboard_name = "Retikon Ops"

use_real_models = true
model_dir = "/app/models"
text_model_name = "BAAI/bge-base-en-v1.5"
image_model_name = "openai/clip-vit-base-patch32"
audio_model_name = "laion/clap-htsat-fused"
whisper_model_name = "small"

ingestion_image = "us-central1-docker.pkg.dev/simitor/retikon-repo/retikon-ingest:latest"
query_image = "us-central1-docker.pkg.dev/simitor/retikon-repo/retikon-query:latest"
audit_image = "us-central1-docker.pkg.dev/simitor/retikon-repo/retikon-audit:latest"
index_image = "us-central1-docker.pkg.dev/simitor/retikon-repo/retikon-query:latest"
dev_console_image = "us-central1-docker.pkg.dev/simitor/retikon-repo/retikon-query:latest"
edge_gateway_image = "us-central1-docker.pkg.dev/simitor/retikon-repo/retikon-edge-gateway:latest"
stream_ingest_image = "us-central1-docker.pkg.dev/simitor/retikon-repo/retikon-stream-ingest:latest"
compaction_image = "us-central1-docker.pkg.dev/simitor/retikon-repo/retikon-compaction:latest"
